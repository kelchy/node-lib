{
  "name": "@kelchy/http-server",
  "version": "0.2.4",
  "scripts": {
    "test:unit": "jest test",
    "lint": "eslint ."
  },
  "pre-commit": {
    "run": [
      "lint",
      "test:unit"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "bail": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "main": "./index.js",
  "license": "GPLv3",
  "dependencies": {
    "@kelchy/common": "^1.0.1",
    "@kelchy/log": "^1.1.4",
    "express": "^4.18.2",
    "cookie-parser": "^1.4.6",
    "body-parser": "^1.20.1",
    "helmet": "^6.0.1",
    "cors": "^2.8.5",
    "swagger-ui-express": "^4.6.0",
    "swagger-jsdoc": "^6.2.7",
    "express-rate-limit": "^6.7.0",
    "express-body-parser-error-handler": "^1.0.4"
  },
  "devDependencies": {
    "@kelchy/pre-commit": "^1.2.4",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "supertest": "^6.2.3",
    "jest": "^29.2.2"
  }
}
